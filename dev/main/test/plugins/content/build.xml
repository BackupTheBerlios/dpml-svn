<?xml version="1.0" encoding="UTF-8" ?>

<project name="dpml-test-content" default="install" basedir="."
    xmlns:transit="antlib:net.dpml.transit" xmlns:x="plugin:dpml/magic/dpml-magic-core">

  <transit:import uri="artifact:template:dpml/magic/standard"/>

  <target name="init" depends="standard.init">
    <x:property key="dpml-metro-central" feature="uri" type="plugin" name="part.plugin"/>
    <filter token="COMPOSITION-CONTENT-PART-HANDLER-PLUGIN-URI" value="${part.plugin}"/>
  </target>

  <target name="package" depends="standard.package">
    <x:export class="net.dpml.test.content.CompositionContentHandlerRegistry"/>
  </target>

</project>
