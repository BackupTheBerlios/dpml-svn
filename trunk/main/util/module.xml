<?xml version="1.0"?>
<module name="util" basedir="."
    xmlns="artifact:xsd:dpml/lang/dpml-module#1.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >

  <dependencies scope="build">
    <include key="transit"/>
  </dependencies>

  <project name="dpml-util-i18n" basedir="i18n">
    <types>
      <type id="jar"/>
    </types>
    <dependencies scope="test">
      <include ref="ant/ant-junit"/>
    </dependencies>
  </project>
   
  <project name="dpml-util-cli" basedir="cli">
    <properties>
      <property name="project.test.fork" value="true"/>
    </properties>
    <types>
      <type id="jar"/>
    </types>
    <dependencies scope="test">
      <include ref="ant/ant-junit"/>
    </dependencies>
  </project>

  <project name="dpml-logging-api" basedir="logging">
    <types>
      <type id="jar"/>
    </types>
  </project>
  
</module>
