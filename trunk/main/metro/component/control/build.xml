<?xml version="1.0" encoding="UTF-8" ?>

<project name="dpml-composition-api" default="install" basedir="." 
    xmlns:transit="antlib:net.dpml.transit" 
    xmlns:x="plugin:dpml/magic/dpml-magic-core" >

  <transit:import uri="local:template:magic/standard"/>

  <target name="init" depends="standard.init">
    <x:filter feature="name" token="NAME"/>
    <x:filter feature="version" token="VERSION"/>
    <x:filter feature="group" token="GROUP"/>
    <x:filter feature="uri" type="plugin" token="COMPOSITION-CONTROLLER-URI" key="dpml-composition-runtime"/>
  </target>

</project>
