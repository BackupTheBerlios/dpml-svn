<?xml version="1.0" encoding="ISO-8859-1"?>

<index>

  <import uri="${dpml.transit.uri}"/>

  <project basedir="info">
    <info>
      <group>dpml-tools</group>
      <name>dpml-tools-info</name>
      <types>
        <type name="jar"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-transit-main"/>
    </dependencies>
  </project>

  <project basedir="model">
    <info>
      <group>dpml-tools</group>
      <name>dpml-tools-model</name>
      <types>
        <type name="jar"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-tools-info"/>
    </dependencies>
  </project>

  <project basedir="control">
    <info>
      <group>dpml-tools</group>
      <name>dpml-tools-control</name>
      <types>
        <type name="jar"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-tools-model"/>
      <include key="ant-junit" runtime="false"/>
    </dependencies>
  </project>

</index>
