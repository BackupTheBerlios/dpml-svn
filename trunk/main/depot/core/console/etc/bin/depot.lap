# 
# This file contains the runtime configuration of the Janel java launcher application.
#

-Djava.system.class.loader=net.dpml.transit.SystemClassLoader
-Djava.security.policy=${env.DPML_HOME}\Shared\bin\security.policy
-Djava.rmi.server.randomIDs=true
janel.main.class=net.dpml.depot.Main
janel.min.java.version=1.4.0
#janel.max.java.version=1.5
janel.classpath.jars.dir=${env.DPML_HOME}\Data\lib
janel.debug.file=${env.DPML_HOME}\Data\logs\janel.log
janel.sysprop.process.id=true
