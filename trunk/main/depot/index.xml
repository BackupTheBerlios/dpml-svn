<?xml version="1.0" encoding="ISO-8859-1"?>

<index key="dpml-depot">

  <import uri="${dpml.transit.uri}"/>
  <import uri="${dpml.magic.uri}"/>
  <import uri="${dpml.metro.uri}"/>

  <!--
  Depot Modules
  -->

  <project file="module.xml">
    <info>
      <group>dpml/depot</group>
      <name>dpml-depot</name>
      <types>
        <type name="module" alias=""/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-transit"/>
    </dependencies>
  </project>

  <!-- Profile -->

  <project basedir="profile/api">
    <info>
      <group>dpml/depot</group>
      <name>dpml-profile-api</name>
      <types>
        <type name="jar"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-transit-main"/>
    </dependencies>
  </project>

  <project basedir="profile/impl">
    <info>
      <group>dpml/depot</group>
      <name>dpml-profile-impl</name>
      <types>
        <type name="jar"/>
        <type name="plugin"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-profile-api" tag="api"/>
      <include key="dpml-station-api" tag="api"/>
    </dependencies>
  </project>

  <!-- Station -->

  <project basedir="console">
    <info>
      <group>dpml/depot</group>
      <name>dpml-depot-console</name>
      <types>
        <type name="jar"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-transit-main"/>
    </dependencies>
  </project>

  <project basedir="prefs">
    <info>
      <group>dpml/depot</group>
      <name>dpml-depot-prefs</name>
      <types>
        <type name="jar"/>
        <type name="plugin"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-profile-api"/>
      <include key="looks" tag="system"/>
    </dependencies>
  </project>

  <project basedir="desktop">
    <info>
      <group>dpml/depot</group>
      <name>dpml-depot-desktop</name>
      <types>
        <type name="jar"/>
        <type name="plugin"/>
      </types>
    </info>
    <dependencies>
      <include key="looks" tag="system"/>
      <include key="dpml-transit-main" tag="system"/>
      <include key="dpml-depot-console" tag="system"/>
      <include key="dpml-part-api" tag="system"/>
      <include key="dpml-profile-api" tag="spi"/>
      <include key="dpml-station-impl"/>
      <include key="forms"/>
    </dependencies>
  </project>

  <project basedir="exec">
    <info>
      <group>dpml/depot</group>
      <name>dpml-depot-exec</name>
      <types>
        <type name="jar"/>
        <type name="plugin"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-transit-main" tag="system"/>
      <include key="dpml-depot-console" tag="system"/>
      <include key="dpml-part-api" tag="system"/>
      <include key="dpml-profile-api" tag="spi" />
      <include key="dpml-station-api" tag="spi"/>
    </dependencies>
  </project>

  <project basedir="build">
    <info>
      <group>dpml/depot</group>
      <name>dpml-depot-build</name>
      <types>
        <type name="jar"/>
        <type name="plugin"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-transit-main"/>
      <include key="dpml-transit-tools"/>
      <include key="dpml-depot-console"/>
      <include key="ant" tag="system"/>
      <include key="ant-launcher" tag="system"/>
      <include key="ant-junit" tag="system"/>
      <include key="junit" tag="system"/>
    </dependencies>
  </project>

  <project basedir="station/api">
    <info>
      <group>dpml/depot</group>
      <name>dpml-station-api</name>
      <types>
        <type name="jar"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-part-api"/>
      <include key="dpml-depot-console"/>
      <include key="dpml-profile-api"/>
    </dependencies>
  </project>

  <project basedir="station/impl">
    <info>
      <group>dpml/depot</group>
      <name>dpml-station-impl</name>
      <types>
        <type name="jar"/>
        <type name="plugin"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-depot-console" tag="system"/>
      <include key="dpml-transit-main" tag="system"/>
      <include key="dpml-part-api" tag="system"/>
      <include key="dpml-profile-api" tag="spi"/>
      <include key="dpml-station-api" tag="spi"/>
    </dependencies>
  </project>

  <project basedir="install">
    <info>
      <group>dpml/depot</group>
      <name>dpml-depot-install</name>
      <types>
        <type name="jar"/>
        <type name="plugin"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-transit-main" tag="system"/>
      <include key="dpml-depot-console" tag="system"/>
      <include key="dpml-station-api" tag="spi"/>
      <include key="dpml-profile-api" tag="spi"/>
      <include key="dpml-profile-impl"/>
    </dependencies>
  </project>

  <project basedir="audit">
    <info>
      <group>dpml/depot</group>
      <name>dpml-depot-audit</name>
      <types>
        <type name="jar"/>
        <type name="plugin"/>
      </types>
    </info>
    <dependencies>
      <include key="dpml-transit-main" tag="system"/>
      <include key="dpml-depot-console" tag="system"/>
      <include key="dpml-profile-api" tag="spi"/>
      <include key="dpml-station-api" tag="spi"/>
    </dependencies>
  </project>

  <resource>
    <info>
      <group>ant</group>
      <name>ant-launcher</name>
      <version>1.6.5</version>
    </info>
  </resource>

</index>
