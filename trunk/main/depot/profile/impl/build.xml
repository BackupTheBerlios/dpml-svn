<?xml version="1.0" encoding="UTF-8" ?>

<project name="dpml-profile-impl" default="install" basedir="." 
    xmlns:transit="antlib:net.dpml.transit" 
    xmlns:x="dpml:tools" >

  <transit:import uri="local:template:tools/standard"/>

  <target name="build" depends="standard.build">
    <rmic base="${project.target.classes.main.dir}" classpathref="project.compile.path">
      <include name="**/ApplicationModel.class"/>
      <include name="**/RegistryModel.class"/>
      <include name="**/StandardRegistryModel.class"/>
    </rmic>
  </target>

</project>
