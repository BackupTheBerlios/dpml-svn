<?xml version="1.0" encoding="ISO-8859-1"?>

<module name="checkstyle" version="3.5">

  <properties>
    <property name="project.api.host" value="http://api.dpml.net"/>
  </properties>

  <resource name="checkstyle">
    <properties>
      <property name="project.api.root" value="${project.api.host}/${group}/${version}"/>
    </properties>
    <types>
      <type id="jar"/>
    </types>
    <dependencies>
      <include ref="antlr/antlr" />
      <include ref="bcel/bcel" />
      <include ref="commons-beanutils/commons-beanutils-core" />
      <include ref="commons-collections/commons-collections" />
      <include ref="commons-logging/commons-logging" />
      <include ref="regexp/regexp" />
    </dependencies>
  </resource>

</module>
